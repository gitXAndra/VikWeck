<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BackgroundImageLayout">
        <attr name="backgroundImage" format="integer"></attr>
        <attr name="allowDistortion" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="DynamicImageView">
        <attr name="fixedDimension" format="enum">
            <enum name="width" value="0"></enum>
            <enum name="height" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>